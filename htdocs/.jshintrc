{
  // Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
  // Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
  // Documentation: http://www.jshint.com/docs/
  "esnext": false,
  "strict": true,
  "globalstrict": true,
  "globals": {
      "zway": true,
      "inherits": true,
      "AutomationControler": true,
      "AutomationModule": true,
      "VirtualDevice": true,
      "$": true,
      "React": true,
      "Immutable": true,
      "_": true,
      "define": true,
      "require": true
  },
  "browser": true,
  "es5" : true,
  "jquery": true,
  "quotmark": false,
  "smarttabs": true,
  "trailing": true,
  "undef": true,
  "unused": true
}
